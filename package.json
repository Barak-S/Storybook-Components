  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn start:dev",
    "start:loc": "webpack-dev-server --mode development --env.ENVFILE=.env.loc",
    "start:dev": "webpack-dev-server --mode development --env.ENVFILE=.env.dev",
    "start:qa": "webpack-dev-server --mode development --env.ENVFILE=.env.qa",
    "start:prd": "webpack-dev-server --mode production --env.ENVFILE=.env.prd",
    "dist": "rm -rf dist/* && webpack -p",
    "dist:dev": "rm -rf dist/* && webpack -p --env.ENVFILE=.env.dev",
    "dist:prd": "rm -rf dist/* && webpack -p --env.ENVFILE=.env.prd",
    "profile:build:prd": "webpack --profile --mode production --env.ENVFILE=.env.prd --json > stats.prd.json",
    "profile:analyse:prd": "webpack-bundle-analyzer stats.prd.json",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint \"src/**\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "clear:cache": "rm -rf ./.cache",
    "postinstall": "yarn clear:cache && husky install"
  },
  "lint-staged": {
    "{*.ts,*tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-amplify/auth": "3.4.22",
