<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <!-- Title -->
    <title><%= htmlWebpackPlugin.options.title %> | <%= htmlWebpackPlugin.options.company %></title>
    <!-- Viewport -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Meta -->
    <meta name=description content="<%= htmlWebpackPlugin.options.description %>"/>
    <meta name="keywords" content="<%= htmlWebpackPlugin.options.keywords.join(', ') %>" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="<%= htmlWebpackPlugin.options.title %>"/>
    <meta property="og:description" content="<%= htmlWebpackPlugin.options.description %>"/>
    <meta property="og:url" content="<%= htmlWebpackPlugin.options.url %>/"/>
    <meta property="og:site_name" content="<%= htmlWebpackPlugin.options.title %>"/>
    <!-- <meta property="og:image" content="/assets/banner.png?v=4"/> -->
    <!-- Favicons -->
    <link rel="shortcut icon" type='image/x-icon' href="<%= htmlWebpackPlugin.options.url %>/favicon.ico">
    <link rel="icon" type="image/png" href="<%= htmlWebpackPlugin.options.url %>/assets/logo32.png">
    <link rel="apple-touch-icon" href="<%= htmlWebpackPlugin.options.url %>/assets/logo180.png">
    <!-- Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" />
</head>
<body>
<div id="app"></div>
</body>
</html>
